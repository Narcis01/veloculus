<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
          prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16"
          fx:controller="com.miele.javafx.controller.SprintCalculatorController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>

    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>

    </rowConstraints>
    <Label styleClass="title" text="Team: " GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    <Label styleClass="title" text="Number of days: " GridPane.halignment="CENTER" GridPane.valignment="CENTER"
           GridPane.rowIndex="1"/>
    <Label styleClass="title" text="Velocity: " GridPane.halignment="CENTER" GridPane.valignment="CENTER"
           GridPane.rowIndex="2"/>
    <Button onAction="#cancel" styleClass="migration" text="Cancel" GridPane.halignment="CENTER" GridPane.rowIndex="3"
            GridPane.valignment="CENTER"/>
    <TextField fx:id="days" GridPane.valignment="CENTER" GridPane.halignment="CENTER" GridPane.columnIndex="1"
               GridPane.rowIndex="1" promptText="ex: 10"/>
    <TextField fx:id="velocity" GridPane.valignment="CENTER" GridPane.halignment="CENTER" GridPane.columnIndex="1"
               GridPane.rowIndex="2" promptText="ex: 40"/>
    <Button onAction="#submit" styleClass="migration" text="Submit" GridPane.columnIndex="2" GridPane.halignment="CENTER"
            GridPane.rowIndex="3" GridPane.valignment="CENTER"/>
    <ComboBox fx:id="comboTeam" GridPane.halignment="CENTER" GridPane.valignment="CENTER" GridPane.rowIndex="0"
              GridPane.columnIndex="1"/>

</GridPane>
